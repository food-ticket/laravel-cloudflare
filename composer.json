{
    "name": "foodticket/laravel-cloudflare",
    "description": "A Laravel wrapper for the Cloudflare API",
    "keywords": ["laravel", "Cloudflare", "api", "Foodticket"],
    "type": "library",
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "cloudflare/sdk": "^1.0.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Foodticket\\Cloudflare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rik Morskate",
            "email": "rik@foodticket.nl",
            "homepage": "https://foodticket.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Foodticket\\Cloudflare\\CloudflareServiceProvider"
            ]
        }
    }
}
